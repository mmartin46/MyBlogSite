@{
}


@model string;

<script>
    var excludedTitle = @Json.Serialize(Model);

    $(document).ready(function () {

        $("#about-section > *:not(" + excludedTitle + ")").css("opacity", 0);
        $("#about-section > *:not(" + excludedTitle + ")").css("position", "relative");

        $("#about-section > *:not(" + excludedTitle + ")").animate({
            top: '+=50px'
        }, 0);
        $("#about-section > *:not(" + excludedTitle + ")").animate({
            opacity: 1,
            top: '-=50px'
        }, 1000);
    });
</script>